        "http-image-size": "1.1.x",
        "lolspeak": "1.4.x",
        "markdown-it": "8.4.x",
        "mehdown-cowsay": "1.1.x",
        "minimist": "1.2.x",
        "natural": "0.5.x",
        "request": "2.83.x",
